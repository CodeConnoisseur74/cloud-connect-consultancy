{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 16,
    "editor.fontFamily": "ComicShannsMono Nerd Font Mono",
    "editor.wordWrap": "on",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.fontWeight": "bold",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeight": "bold",
    "editor.formatOnSave": true,
    "editor.mouseWheelZoom": true,
    "window.zoomLevel": 1,
    "terminal.integrated.mouseWheelZoom": true,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiRed",
    "terminal.integrated.fontFamily": "ComicShannsMono Nerd Font Mono",
    "powermode.enabled": true,
    "powermode.combo.location": "off",
    "powermode.combo.counterEnabled": "hide",
    "powermode.combo.timerEnabled": "hide",
    "powermode.shake.intensity": 0,
    "workbench.colorTheme": "Sax Synth Color Theme",
    "workbench.iconTheme": "sweet-vscode-icons",
    "python.terminal.activateEnvironment": false,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "javascript.format.semicolons": "insert",
    "javascript.format.enable": true,
    "eslint.format.enable": true,
    "[css]": {
        "editor.defaultFormatter": "aeschli.vscode-css-formatter"
    },
    "workbench.colorCustomizations": {
        "minimap.background": "#00000000",
        "scrollbar.shadow": "#00000000",
        "terminal.foreground": "#fc96ff",
        "terminalCursor.foreground": "#7e4bb8",
    },
    "glassit-linux.opacity": 95,
    "editor.experimentalInlineEdit.backgroundColoring": true,
    "animations.CursorAnimation": true,
    "animations.CursorAnimationOptions": {
        "Color": "#7e4bb8"
    },
    "editor.cursorBlinking": "smooth",
    "animations.Custom-CSS": false,
    "animations.Default-Duration": 500,
    "animations.Auto-Install": true,
    "editor.cursorSurroundingLines": 5,
    "animations.Install-Method": "Apc Customize UI++",
    "animations.Focus-Dimming-Mode": "Full Window",
    "animations.Tabs": "Scale",
    "workbench.editor.highlightModifiedTabs": false,
    "editor.linkedEditing": true,
    "terminal.integrated.copyOnSelection": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "workbench.editor.autoLockGroups": {
        "vscode.audioPreview": true
    },
}